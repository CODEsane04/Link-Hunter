{   "manifest_version": 3,
    "name" : "visual Product Search",
    "version" : "1.0",
    "description" : "visual Product Search",
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "action": {
    "default_popup": "vps.html",
    "default_title": "My First Extension",
    "default_icon": {
            "16": "icons/lhicon16.png",
            "32": "icons/lhicon32.png",
            "48": "icons/lhicon48.png",
            "128": "icons/lhicon128.png"
        }
    },
    "icons": {
    "16": "icons/lhicon16.png",
    "32": "icons/lhicon32.png",
    "48": "icons/lhicon48.png",
    "128": "icons/lhicon128.png"
  }


}